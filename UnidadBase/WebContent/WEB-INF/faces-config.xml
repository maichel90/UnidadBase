<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>confBean</managed-bean-name>
  <managed-bean-class>co.themafia.backBean.Configuracion</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sensorBean</managed-bean-name>
  <managed-bean-class>co.themafia.backBean.SensorBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>consultarMedicamentos</managed-bean-name>
  <managed-bean-class>co.themafia.backBean.ConsultarMedicamentos</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>consultaSensores</managed-bean-name>
  <managed-bean-class>co.themafia.backBean.ConsultaSensores</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/*</from-view-id>
  <navigation-case>
   <from-outcome>configuracion</from-outcome>
   <to-view-id>/xhtml/Configuracion.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>csensores</from-outcome>
   <to-view-id>/xhtml/ConsultaSensores.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cmedicamentos</from-outcome>
   <to-view-id>/xhtml/ConsultaMedicamentos.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>simulacion</from-outcome>
   <to-view-id>/xhtml/Simulacion.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
